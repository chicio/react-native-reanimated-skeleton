{
  "homepage": "https://marcuzgabriel.github.io/react-native-reanimated-skeleton/",
  "name": "react-native-reanimated-skeleton",
  "author": "Marcuz Gabriel Larsen <marcuzgabriel@gmail.com>",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "align-deps": "yarn rnx-align-deps",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "type:check": "yarn tsc --noEmit",
    "type:generate:tsc": "yarn tsc",
    "type:generate:ios": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:ios && yarn type:generate:copy:ios",
    "type:generate:clean": "cross-env-shell rimraf lib",
    "type:generate:create": "cross-env-shell mkdir lib",
    "type:generate:delete:ios": "rm ./lib/ReactNativeReanimatedSkeleton.js",
    "type:generate:copy:ios": "rsync -auv ./src/ReactNativeReanimatedSkeleton.ts ./lib/index.js"
  },
  "react-native": "src/ReactNativeReanimatedSkeleton.ts",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "react-native-reanimated-skeleton.d.ts",
  "files": [
    "src/",
    "lib/",
    "react-native-reanimated-skeleton.d.ts",
    "README.md",
    "!__snapshots__",
    "!*.test.js"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-linear-gradient": "*",
    "react-native-reanimated": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-arrow-functions": "^7.22.5",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@rnx-kit/align-deps": "^2.2.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-test-renderer": "^18.0.1",
    "babel-jest": "^29.6.4",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.6.4",
    "jest-sonar-reporter": "^2.0.0",
    "metro-config": "^0.79.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "semantic-release": "21.1.1",
    "semantic-release-cli": "^5.4.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.72"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "clipboard",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "masked-view",
        "metro-config",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcuzgabriel/react-native-reanimated-skeleton.git"
  }
}
